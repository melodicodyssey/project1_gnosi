<h1>Welome!  Please Sign In</h1>
<%= button_to "Sign Up", new_user_path, method: 'get' %>
<%= form_for(:session, url: create_session_path) do |f| %>
	<%= f.label :email %>
	<%= f.text_field :email %></br>
	<%= f.label :password %>
	<%= f.password_field :password %></br>
	<%= f.submit "Login" %>
<% end %>

